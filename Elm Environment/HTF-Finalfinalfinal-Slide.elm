piechart data start = group (createPie data start)

createPie data start = case data of
              (x,c) :: xs -> (wedge 100 x
                                |> filled c
                                |> rotate (degrees (start + 180 * x))
                                ) :: createPie xs (start + 360 * x)
              _ -> []

